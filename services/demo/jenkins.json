{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "jenkins",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "NAME": "jenkins",
    "ENVIRONMENT": "prod",
    "SERVICE_8080_NAME": "jenkins"
  },
  "id": "jenkins",
  "instances": 1,
  "cpus": 0.5,
  "mem": 512
}
